val input = "Coronaoutbreak.csv"

val RDD = sc.textFile(input)

val lineRDD = RDD.map(line=>line.split(";"))

val dataRDD = lineRDD.map(field=>field(1))

val dateWithoutClockRDD = dataRDD.map(line=>line.split(" ")).map(field=>field(0))

val countDateRDD = dateWithoutClockRDD.map(field=>(field, 1))

val finalRDD = countDateRDD.reduceByKey((v1, v2)=>v1 + v2)

val output = "Coronaoutbreak_DateCount"

finalRDD.saveAsTextFile(output)
